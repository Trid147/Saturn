	local function playSound()
		local sound = Instance.new("Sound")
		sound.SoundId = ClickSound
		sound.Parent = player
		sound.Volume = 25
		sound:Play()
		spawn(function()  
			wait(sound.TimeLength)
			sound:Destroy()
		end)
	end

	local function secretPlay() 
		local sound = Instance.new("Sound")
		sound.SoundId = SecretSound
		sound.Parent = player
		sound.Volume = 25
		sound:Play()
		spawn(function()  
			wait(sound.TimeLength)
			sound:Destroy()
		end)
	end

