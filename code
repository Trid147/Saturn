					AimRealButton = Enum.KeyCode.I
				elseif AimButtonVvod.ContentText == "K" then
					AimRealButton = Enum.KeyCode.K
				elseif AimButtonVvod.ContentText == "J" then
					AimRealButton = Enum.KeyCode.J
				elseif AimButtonVvod.ContentText == "L" then
					AimRealButton = Enum.KeyCode.L
				elseif AimButtonVvod.ContentText == "H" then
					AimRealButton = Enum.KeyCode.H
				elseif AimButtonVvod.ContentText == "R" then
					AimRealButton = Enum.KeyCode.R
				elseif AimButtonVvod.ContentText == "V" then
					AimRealButton = Enum.KeyCode.V
				elseif AimButtonVvod.ContentText == "G" then
					AimRealButton = Enum.KeyCode.G
				elseif AimButtonVvod.ContentText == "H" then
					AimRealButton = Enum.KeyCode.H
				elseif AimButtonVvod.ContentText == "E" then
					AimRealButton = Enum.KeyCode.E
				elseif AimButtonVvod.ContentText == "M" then
					AimRealButton = Enum.KeyCode.M
				elseif AimButtonVvod.ContentText == "N" then
					AimRealButton = Enum.KeyCode.N
				elseif AimButtonVvod.ContentText == "Q" then
					AimRealButton = Enum.KeyCode.Q
				elseif AimButtonVvod.ContentText == "U" then
					AimRealButton = Enum.KeyCode.U
				elseif AimButtonVvod.ContentText == "B" then
					AimRealButton = Enum.KeyCode.B
				else
					print(AimButtonVvod.ContentText)
				end

				if FlashlightButtonVvod.ContentText == "F" then
					FlashlightRealButton = Enum.KeyCode.F
				elseif FlashlightButtonVvod.ContentText == "T" then
					FlashlightRealButton = Enum.KeyCode.T
				elseif FlashlightButtonVvod.ContentText == "Y" then
					FlashlightRealButton = Enum.KeyCode.Y
				elseif FlashlightButtonVvod.ContentText == "I" then
					FlashlightRealButton = Enum.KeyCode.I
				elseif FlashlightButtonVvod.ContentText == "K" then
					FlashlightRealButton = Enum.KeyCode.K
				elseif FlashlightButtonVvod.ContentText == "J" then
					FlashlightRealButton = Enum.KeyCode.J
				elseif FlashlightButtonVvod.ContentText == "L" then
					FlashlightRealButton = Enum.KeyCode.L
				elseif FlashlightButtonVvod.ContentText == "H" then
					FlashlightRealButton = Enum.KeyCode.H
				elseif FlashlightButtonVvod.ContentText == "R" then
					FlashlightRealButton = Enum.KeyCode.R
				elseif FlashlightButtonVvod.ContentText == "V" then
					FlashlightRealButton = Enum.KeyCode.V
				elseif FlashlightButtonVvod.ContentText == "G" then
					FlashlightRealButton = Enum.KeyCode.G
				elseif FlashlightButtonVvod.ContentText == "H" then
					FlashlightRealButton = Enum.KeyCode.H
				elseif FlashlightButtonVvod.ContentText == "E" then
					FlashlightRealButton = Enum.KeyCode.E
				elseif FlashlightButtonVvod.ContentText == "M" then
					FlashlightRealButton = Enum.KeyCode.M
				elseif FlashlightButtonVvod.ContentText == "N" then
					FlashlightRealButton = Enum.KeyCode.N
				elseif FlashlightButtonVvod.ContentText == "Q" then
					FlashlightRealButton = Enum.KeyCode.Q
				elseif FlashlightButtonVvod.ContentText == "U" then
					FlashlightRealButton = Enum.KeyCode.U
				elseif FlashlightButtonVvod.ContentText == "B" then
					FlashlightRealButton = Enum.KeyCode.B
				else
					print(FlashlightButtonVvod.ContentText)
				end

				if FlyButtonVvod.ContentText == "F" then
					FlyRealButton = Enum.KeyCode.F
				elseif FlyButtonVvod.ContentText == "T" then
					FlyRealButton = Enum.KeyCode.T
				elseif FlyButtonVvod.ContentText == "Y" then
					FlyRealButton = Enum.KeyCode.Y
				elseif FlyButtonVvod.ContentText == "I" then
					FlyRealButton = Enum.KeyCode.I
				elseif FlyButtonVvod.ContentText == "K" then
					FlyRealButton = Enum.KeyCode.K
