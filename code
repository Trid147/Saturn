	local FlashlightActive = false

	local Noclip = false
	local FlyJump = false
	local SpeedHack = false
	local Aimbot = false
	local ESP = false
	local CtrlClickTP = false
	local Flashlight = false
	local Fly = false

	local FirstSpeed = false
	local SecondSpeed = false
	local ThirdSpeed = false

	local function OffCheat()
		Noclip = false
		SpeedHack = false
		ESP = false
		CtrlClickTP = false
		FlyJump = false
		Aimbot = false
		Flashlight = false
		Fly = false

		FirstSpeed = false
		SecondSpeed = false
		ThirdSpeed = false

		player.Character.UpperTorso.CanCollide = true
		player.Character.LowerTorso.CanCollide = true
		player.Character.HumanoidRootPart.CanCollide = true

		player.Character.Humanoid.WalkSpeed = 16

		NoclipStatus.BackgroundColor3 = Color3.new(0.666667, 0, 0)
		SpeedHackStatus.BackgroundColor3 = Color3.new(0.666667, 0, 0)
		ESPStatus.BackgroundColor3 = Color3.new(0.666667, 0, 0)
		CtrlClickTPStatus.BackgroundColor3 = Color3.new(0.666667, 0, 0)
		FlyJumpStatus.BackgroundColor3 = Color3.new(0.666667, 0, 0)
		AimbotStatus.BackgroundColor3 = Color3.new(0.666667, 0, 0)
		FlashlightStatus.BackgroundColor3 = Color3.new(0.666667, 0, 0)
		FlyStatus.BackgroundColor3 = Color3.new(0.666667, 0, 0)
	end

	local function TurnPage()
		if PageStatus1.Value == true then

			PageStatus1.Value = false
			PageStatus2.Value = true

			for k,v in Page1:GetChildren() do
				if not v:IsA("BoolValue") then
					v.Visible = false
				end
			end

			for k,v in Page2:GetChildren() do
				if not v:IsA("BoolValue") then
					v.Visible = true
				end
			end

		else

			PageStatus1.Value = true
			PageStatus2.Value = false

			for k,v in Page1:GetChildren() do
				if not v:IsA("BoolValue") then
					v.Visible = true
				end
			end

			for k,v in Page2:GetChildren() do
				if not v:IsA("BoolValue") then
					v.Visible = false
				end
			end

		end
	end

	Cross.MouseButton1Click:Connect(function()
		CheatOn = false
		SaturnCheat:Destroy()
		OffCheat()
		playSound()
		print("Saturn Cheat has been closed")
		script:Destroy()
	end)

