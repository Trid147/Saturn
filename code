					FlyRealButton = Enum.KeyCode.H
				elseif FlyButtonVvod.ContentText == "R" then
					FlyRealButton = Enum.KeyCode.R
				elseif FlyButtonVvod.ContentText == "V" then
					FlyRealButton = Enum.KeyCode.V
				elseif FlyButtonVvod.ContentText == "G" then
					FlyRealButton = Enum.KeyCode.G
				elseif FlyButtonVvod.ContentText == "H" then
					FlyRealButton = Enum.KeyCode.H
				elseif FlyButtonVvod.ContentText == "E" then
					FlyRealButton = Enum.KeyCode.E
				elseif FlyButtonVvod.ContentText == "M" then
					FlyRealButton = Enum.KeyCode.M
				elseif FlyButtonVvod.ContentText == "N" then
					FlyRealButton = Enum.KeyCode.N
				elseif FlyButtonVvod.ContentText == "Q" then
					FlyRealButton = Enum.KeyCode.Q
				elseif FlyButtonVvod.ContentText == "U" then
					FlyRealButton = Enum.KeyCode.U
				elseif FlyButtonVvod.ContentText == "B" then
					FlyRealButton = Enum.KeyCode.B
				else
					print(FlashlightButtonVvod.ContentText)
				end
			end
		end
	end)

	UserInputService.InputBegan:Connect(function(input, isTyping)
		if isTyping then return end
		if CheatOn then
			if input.KeyCode == AimRealButton then
				if Aimbot then
					if not AimbotActive then
						AimbotActive = true
						AimbotActiveG.BackgroundColor3 = Color3.new(0, 0.666667, 0)
						aimbotConnection = RunService.RenderStepped:Connect(updateAimbot)
					else
						AimbotActive = false
						AimbotActiveG.BackgroundColor3 = Color3.new(0.666667, 0, 0)
					end
				end
			end
			if input.KeyCode == FlashlightRealButton then
				if Flashlight then
					if not FlashlightActive then
						FlashlightActive = true
						FlashlightActiveG.BackgroundColor3 = Color3.new(0, 0.666667, 0)
						SpawnLight()
					else
						FlashlightActive = false
						FlashlightActiveG.BackgroundColor3 = Color3.new(0.666667, 0, 0)
						player.Character.Head:FindFirstChild("Flashlight"):Destroy()
					end
					PlayFlashlightClick()
				end
			end
			if input.KeyCode == FlyRealButton then
				Fly = not Fly
				if Fly then
					FlyStatus.BackgroundColor3 = Color3.new(0, 0.666667, 0)
				else
					FlyStatus.BackgroundColor3 = Color3.new(0.666667, 0, 0)
				end
				FlyProcess()
			end
		end
	end)

	local defaultHighlightColor = Color3.fromRGB(255, 255, 255)
	local outlineColor = Color3.fromRGB(0, 0, 0) 
	local fillTransparency = 0.5
	local outlineTransparency = 0 
	local maxDistance = 1000

	local playerDistances = {}

	local function getHighlightColor(player)
		local team = player.Team
		if team then
			return team.TeamColor.Color
		else
			return defaultHighlightColor
		end
	end

	local function createHighlight(player)
		local character = player.Character or player.CharacterAdded:Wait()
		if character and not character:FindFirstChild("PlayerHighlight") and CheatOn then
			local highlight = Instance.new("Highlight")
			highlight.Name = "PlayerHighlight"
			highlight.Adornee = character
			highlight.FillColor = getHighlightColor(player)
			highlight.OutlineColor = outlineColor
			highlight.FillTransparency = fillTransparency
			highlight.OutlineTransparency = outlineTransparency
			highlight.Parent = character
		end
	end

	local function removeHighlight(player)
		local character = player.Character
		if character then
			local highlight = character:FindFirstChild("PlayerHighlight")
			if highlight and CheatOn then
				highlight:Destroy()
			end
		end
	end

	local function updateHighlights()
		local localPlayer = Players.LocalPlayer
		local localCharacter = localPlayer.Character
		if localCharacter and localCharacter.PrimaryPart then
			local localPosition = localCharacter.PrimaryPart.Position
			for _, player in pairs(Players:GetPlayers()) do
				if player ~= localPlayer then
					local character = player.Character
					if character and character.PrimaryPart and CheatOn then
						local distance = (localPosition - character.PrimaryPart.Position).Magnitude
						playerDistances[player] = distance
						if distance <= maxDistance then
							createHighlight(player)
						else
							removeHighlight(player)
						end
					end
				end
			end
		end
	end

	local function batchUpdateHighlights()
		for _, player in pairs(Players:GetPlayers()) do
			if ESP and playerDistances[player] and playerDistances[player] <= maxDistance then
				createHighlight(player)
			else
				removeHighlight(player)
			end
		end
	end

	Players.PlayerAdded:Connect(function(player)
		player.CharacterAdded:Connect(function()
			if ESP then
				createHighlight(player)
			end
		end)
		player.CharacterRemoving:Connect(function()
			removeHighlight(player)
		end)
	end)

	Players.PlayerRemoving:Connect(function(player)
		removeHighlight(player)
		playerDistances[player] = nil
	end)

	spawn(function()
		while true do
			if ESP then
				updateHighlights()
			end
			wait(0.5)
		end
	end)

	spawn(function()
		while true do
			batchUpdateHighlights()
			wait(0.1)
		end
	end)

	CtrlClickTPButton.MouseButton1Click:Connect(function()  
		if CheatOn then
			if not CtrlClickTP then
				CtrlClickTP = true
				CtrlClickTPStatus.BackgroundColor3 = Color3.new(0, 0.666667, 0)
				playSound()
			else
				CtrlClickTP = false
				CtrlClickTPStatus.BackgroundColor3 = Color3.new(0.666667, 0, 0)
				playSound()
			end
		end
	end)

	function GetCharacter() 
		return Players.LocalPlayer.Character 
	end

	function Teleport(pos) 
		local Char = GetCharacter() 
		if Char then 
			Char:MoveTo(pos) 
		end 
	end

	UserInputService.InputBegan:Connect(function(input)
		if CheatOn then
			if CtrlClickTP then
				if input.UserInputType == Enum.UserInputType.MouseButton1 and UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
					Teleport(mouse.Hit.p) 
				end 
			end
		end
	end)

	local char = player.Character

	local function onCharAdded()
		wait(1)
		char = player.Character
		spawn(function()  
			while char do
				if CheatOn then
					if SpeedHack then
						if FirstSpeed then
							char.Humanoid.WalkSpeed = 16
						elseif SecondSpeed then
							char.Humanoid.WalkSpeed = tonumber(MediumSpeedVvod.ContentText)
						elseif ThirdSpeed then
							char.Humanoid.WalkSpeed = 200
						end
					end
				end
				task.wait()
			end
		end)

		spawn(function()  
			while char do
				if CheatOn then
					if Noclip then
						for _, part in pairs(char:GetDescendants()) do
							if part:IsA("BasePart") and part.CanCollide then
								part.CanCollide = false
							end
						end
					else
						for _, part in pairs(char:GetDescendants()) do
							if part:IsA("BasePart") then
								if part.Name == "LowerTorso" or part.Name == "UpperTorso" or part.Name == "HumanoidRootPart" then
									part.CanCollide = true
								end
							end
						end
					end
				end
				task.wait()
			end
		end)
	end

	player.CharacterAdded:Connect(onCharAdded)

	player.Character.Humanoid.Died:Connect(function()
		FirstSpeed = false
		SecondSpeed = false
		ThirdSpeed = false
		Noclip = false
	end)

	onCharAdded()

else
	local sound = Instance.new("Sound")
	sound.SoundId = BLOCK_SOUND
	sound.Parent = game.Players.LocalPlayer
	sound.Volume = 25
	sound:Play()
	sound:Play()
	script:Destroy()
end
